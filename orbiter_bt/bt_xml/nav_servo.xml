<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <SetBlackboard output_key="arucoID" value="aruco1"/>
            <Action ID="moveHead" head_goal="{arucoID}"/>
            <Action ID="getItemPose" itemname="{arucoID}" pose="{aruco_pose}" type="drop"/>
            <Action ID="NavFallback" type="aruco" pose="{aruco_pose}" fwd_dist="0.5"/>
            <AlwaysFailure/>
        </Sequence>
    </BehaviorTree>
</root>